              },
              "region_name" : {
                "type" : "string",
				"index": "not_analyzed"
              },
              "timezone" : {
                "type" : "string"
              }
            }
          },
          "host" : {
            "type" : "string",
			"index": "not_analyzed"
          },
          "AgentName" : {
            "type" : "string",
			"index": "not_analyzed"
