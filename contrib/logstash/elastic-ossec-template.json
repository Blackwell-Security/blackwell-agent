{
    "order" : 0,
    "template" : "ossec*",
    "settings" : {
      "index.refresh_interval" : "5s"
    },
    "mappings" : {
	"ossec" : {
		  "dynamic_templates": [
            { "notanalyzed": {
                  "match":              "*", 
                  "match_mapping_type": "string",
                  "mapping": {
                      "type":        "string",
                      "index":       "not_analyzed"
                  }
               }
            }
          ],
        "properties" : {
          "@timestamp" : {
            "type" : "date",
            "format" : "dateOptionalTime"
          },
          "@version" : {
            "type" : "string"
          },
          "AgentIP" : {
            "type" : "string",
			"index": "not_analyzed"
          },
          "dstuser" : {
            "type" : "string",
			"index": "not_analyzed"
          },
          "AlertsFile" : {
            "type" : "string",
			"index": "not_analyzed"
          },
