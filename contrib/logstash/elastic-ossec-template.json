                "type" : "string",
				"index": "not_analyzed"
              },
			  "md5_before" : {
                "type" : "string",
				"index": "not_analyzed"
              }
			 }
          },
          "location" : {
            "type" : "string",
			"index": "not_analyzed"
          },
          "message" : {
            "type" : "string"
          },
          "offset" : {
            "type" : "string"
          },
          "rule" : {
            "properties" : {
              "description" : {
                "type" : "string",
