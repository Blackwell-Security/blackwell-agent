include "common_agentInfo.fbs";

table syscollector_ports
{
    attributes_type: string;
    attributes: PortAttributeMsg;
    index: string;
    timestamp:string;
}

table PortAttributeMsg
{
    checksum: string;
    inode: int;
    item_id: string;
    local_ip: string;
    local_port: int;
    pid: int;
    process: string;
    protocol: string;
    remote_ip: string;
    remote_port: int;
    rx_queue: int;
    scan_time: string;
    state: string;
    tx_queue: int;
}
